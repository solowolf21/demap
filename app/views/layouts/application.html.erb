<!DOCTYPE html>
<html>
<head>
  <title>Demap</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/header' %>
<%= render 'layouts/sidebar' %>

<% if flash[:notice] %>
    <div class='flash-notice'>
      <%= flash[:notice] %>
    </div>
<% end %>

<div style="padding-left: 5em">
    <%= yield %>
</div>

<%= render 'layouts/footer' %>
</body>
</html>
